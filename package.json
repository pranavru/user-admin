    "@typescript-eslint/parser": "^8.25.0",
    "dotenv": "^16.0.0",
    "eslint": "^9.21.0",
    "globals": "^16.0.0",
    "nodemon": "^3.1.9",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2",
