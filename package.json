{
  "name": "samanvay-admin",
  "version": "1.0.0",
  "description": "Admin app to store and manage data for Samanvay Foundation",
  "main": "dist/app.js",
  "scripts": {
    "start": "node dist/app.js",
    "dev": "NODE_ENV=development nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/app.ts",
    "staging": "NODE_ENV=staging ts-node src/app.ts",
    "prod": "NODE_ENV=production ts-node src/app.ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "rm -rf dist && tsc"
  },
  "dependencies": {
    "express": "4.21.2",
    "mongoose": "8.11.0"
  },
  "devDependencies": {
    "@types/express": "5.0.0",
    "@types/node": "22.13.8",
    "dotenv": "16.0.0",
    "globals": "16.0.0",
    "nodemon": "3.1.9",
    "ts-node": "10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.8.2"
  },
  "keywords": [],
  "author": "Pranav Ruparelia",
  "license": "ISC"
}
